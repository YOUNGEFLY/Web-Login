<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
    <welcome-file>resources/templates/login.jsp</welcome-file>
  </welcome-file-list>


  <servlet>
    <servlet-name>UserPswLogin</servlet-name>
    <servlet-class>Servlet.UserPswLoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserPswLogin</servlet-name>
    <url-pattern>/UserPswLogin</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Register</servlet-name>
    <servlet-class>Servlet.RegisterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Register</servlet-name>
    <url-pattern>/Register</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Home</servlet-name>
    <servlet-class>Servlet.HomeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Home</servlet-name>
    <url-pattern>/Home</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>SendSmsCode</servlet-name>
    <servlet-class>Servlet.SendSMSCodeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SendSmsCode</servlet-name>
    <url-pattern>/SendSmsCode</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>AlterUserNameServlet</servlet-name>
    <servlet-class>Servlet.AlterUser.AlterUserNameServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AlterUserNameServlet</servlet-name>
    <url-pattern>/AlterUserName</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>AlterUserPswServlet</servlet-name>
    <servlet-class>Servlet.AlterUser.AlterUserPswServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AlterUserPswServlet</servlet-name>
    <url-pattern>/AlterUserPsw</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>DbaLoginServlet</servlet-name>
    <servlet-class>Servlet.DbaLoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DbaLoginServlet</servlet-name>
    <url-pattern>/DbaLogin</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>LoadUserServlet</servlet-name>
    <servlet-class>Servlet.LoadUserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoadUserServlet</servlet-name>
    <url-pattern>/LoadUser</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>DelUserServlet</servlet-name>
    <servlet-class>Servlet.DelUserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DelUserServlet</servlet-name>
    <url-pattern>/DelUser</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>AlterUserServlet</servlet-name>
    <servlet-class>Servlet.AlterUserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AlterUserServlet</servlet-name>
    <url-pattern>/AlterUser</url-pattern>
  </servlet-mapping>
</web-app>
